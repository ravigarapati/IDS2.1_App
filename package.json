    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-svg-transformer": "^1.0.0",
    "jetifier": "^1.6.6",
    "jsdoc": "^3.6.6",
    "jsdoc-to-markdown": "^6.0.1",
    "metro": "^0.79.1",
