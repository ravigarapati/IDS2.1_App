    "obfuscator-io-metro-plugin": "^2.1.3",
    "react-test-renderer": "17.0.2",
    "ts-lint": "^4.5.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "setupFiles": [
      "./__tests__/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ],
    "coverageDirectory": "./__tests__/test-reports",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "preset": "react-native",
    "testMatch": [
      "**/__tests__/spec/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "name": "IDSMobileApp",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/IDSMobileApp/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest --coverage",
    "sonar": "jest --coverage && sonar-scanner",
    "test:coverage": "react-scripts test --coverage",
    "jsdoc": "jsdoc -c jsdoc.json",
    "postinstall": "jetify",
    "androidprod": "npm run androidprebuild && npm run androidbuild",
    "androidprebuild": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "androidbuild": "cd android && gradlew clean && gradlew app:bundleRelease && cd ..",
    "release-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew assembleRelease && cd .."
  },
  "version": "1.2.1"
}
